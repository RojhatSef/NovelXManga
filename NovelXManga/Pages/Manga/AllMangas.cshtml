@page
@model NovelXManga.Pages.Manga.AllMangasModel
@{
  
}

<div>Hello world</div>
@foreach (var Books in Model.GetAllBooks)
{
    
    <div class="row">
    </div>
  

    @if (Books.MangaName != null)
    {
        <div class="mb-3">
            <label class="form-label" asp-for="@Books.MangaName"> @Books.MangaName</label>

        </div>
    }
