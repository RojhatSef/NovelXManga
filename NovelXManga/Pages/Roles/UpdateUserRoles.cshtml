@page
@model NovelXManga.Pages.Roles.UpdateUserRolesModel
@{
}
@{
    ViewData["Title"] = "READ USER";
}

<h1 class="bg-info text-white">All Users</h1>
<a asp-action="Create" class="btn btn-secondary">Create a User</a>
@foreach (UserModel user in Model.usersList)
    {
<table class="table table-sm table-bordered">
    <tr><th>ID</th><th>Name</th></tr>
  
        <tr>
            <td>@user.Id</td>
       
            <td>@user.Email</td>
        @*    <td>@user.Discriminator </td>*@
        </tr>





    </table>
    <input type="checkbox" asp-route-id="@user.Id">
}